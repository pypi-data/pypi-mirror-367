include README.md
include LICENSE
include requirements.txt
include pyproject.toml
include package.json
include yarn.lock
include tsconfig.json
include src/**/*
include style/**/*
include reprolab/labextension/**/*
recursive-include reprolab *.py
recursive-exclude * __pycache__
recursive-exclude * *.py[co]

# Exclude virtual environments
exclude my_venv/
exclude .my_venv/
exclude *venv*/
exclude *env*/

# Exclude development files
exclude reprolab_data/
exclude node_modules/
exclude dist/
exclude build/
exclude *.egg-info/
exclude .git/
exclude .gitignore
exclude .DS_Store
exclude .idea/
exclude .ipynb_checkpoints/
exclude .virtual_documents/
exclude .yarn/
exclude .yarnrc.yml
exclude .prettierignore
exclude .copier-answers.yml

# Exclude test files and notebooks
exclude *.ipynb
exclude test_*.ipynb
exclude demo*.ipynb
exclude Untitled*.ipynb
exclude books.csv
exclude demo.md
exclude demo2.ipynb

# Exclude build artifacts
exclude lib/
exclude tsconfig.tsbuildinfo
exclude package-lock.json 
