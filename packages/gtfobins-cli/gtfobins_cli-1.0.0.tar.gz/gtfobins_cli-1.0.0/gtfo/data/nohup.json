{
  "functions": {
    "shell": [
      {
        "code": "nohup /bin/sh -c \"sh <$(tty) >$(tty) 2>$(tty)\""
      }
    ],
    "command": [
      {
        "code": "nohup \"[command]\"\ncat nohup.out\n"
      }
    ],
    "sudo": [
      {
        "code": "sudo nohup /bin/sh -c \"sh <$(tty) >$(tty) 2>$(tty)\""
      }
    ],
    "suid": [
      {
        "code": "./nohup /bin/sh -p -c \"sh -p <$(tty) >$(tty) 2>$(tty)\""
      }
    ]
  }
}
