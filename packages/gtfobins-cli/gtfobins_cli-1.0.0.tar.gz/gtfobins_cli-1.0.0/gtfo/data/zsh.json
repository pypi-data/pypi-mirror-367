{
  "functions": {
    "file-read": [
      {
        "code": "zsh -c 'echo \"$(<[file])\"'\n"
      }
    ],
    "file-write": [
      {
        "code": "zsh -c 'echo [data] >[file]'\n"
      }
    ],
    "shell": [
      {
        "code": "zsh"
      }
    ],
    "suid": [
      {
        "code": "./zsh"
      }
    ],
    "sudo": [
      {
        "code": "sudo zsh"
      }
    ]
  }
}
