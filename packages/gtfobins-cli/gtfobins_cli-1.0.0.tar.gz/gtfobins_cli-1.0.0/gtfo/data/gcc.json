{
  "functions": {
    "file-read": [
      {
        "code": "gcc -x c -E \"[file]\"\n"
      }
    ],
    "file-write": [
      {
        "code": "gcc -xc /dev/null -o [file]\n"
      }
    ],
    "shell": [
      {
        "code": "gcc -wrapper /bin/sh,-s ."
      }
    ],
    "sudo": [
      {
        "code": "sudo gcc -wrapper /bin/sh,-s ."
      }
    ]
  }
}
