# GCP Configuration
GCP_PROJECT_ID=.....
GCP_REGION=us-central1
GCP_CLUSTER_NAME=.....

# Domain Configuration
DOMAIN=a2a-registry.dev
API_SUBDOMAIN=api
REGISTRY_SUBDOMAIN=registry

# Deployment Configuration
USE_CLOUDFLARE=true
ENABLE_DIRECT_SSL=false

# Docker Configuration
DOCKER_REGISTRY=gcr.io
DOCKER_IMAGE_NAME=a2a-registry

# Storage Configuration
STORAGE_TYPE=file
STORAGE_DATA_DIR=/data

# Optional: Service Account Key (for local development only)
# GCP_SA_KEY_PATH=path/to/service-account-key.json 