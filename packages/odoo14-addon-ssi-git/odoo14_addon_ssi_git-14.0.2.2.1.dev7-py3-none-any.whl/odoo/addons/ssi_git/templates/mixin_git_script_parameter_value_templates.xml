<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
<template id="parameter_value">
    <page
            string="Parameter Values"
            name="parameter_value"
            groups="ssi_git.git_script_group"
        >
        <button
                name="action_populate_parameter_value_ids"
                type="object"
                string="Populate"
                class="oe_highlight"
                colspan="2"
            />
        <field name='parameter_value_ids'>
            <tree editable="bottom">
                <field name="script_id" />
                <field name="parameter_id" />
                <field name="type" />
                <field name="value" />
            </tree>
        </field>
    </page>
</template>
</odoo>
