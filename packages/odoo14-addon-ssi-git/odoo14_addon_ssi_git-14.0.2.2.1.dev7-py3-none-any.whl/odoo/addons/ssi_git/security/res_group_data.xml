<odoo>
<!-- Configurator -->
<record id="git_repo_type_group" model="res.groups">
    <field name="name">Git Repository TYpe</field>
    <field name="category_id" ref="git_configurator_module_category" />
    <field
            name="users"
            eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"
        />
</record>

