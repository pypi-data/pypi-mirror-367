{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File - Cache Middleware",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "Pytest - Cache Middleware",
      "type": "debugpy",
      "request": "launch",
      "module": "pytest",
      "cwd": "${workspaceRoot}/",
      "args": ["tests"]
    },
    {
      "name": "MyPy - Cache Middleware",
      "type": "debugpy",
      "request": "launch",
      "module": "mypy",
      "cwd": "${workspaceRoot}/",
      "args": ["--ignore-missing-imports", "src/"]
    },
    {
      "name": "Examples - Cache Middleware",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceRoot}/examples/backend_examples.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceRoot}/examples",
      "justMyCode": true,
      "python": "${workspaceRoot}/.venv/Scripts/python.exe"
    }
  ]
}
