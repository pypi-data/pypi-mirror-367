{
    "log": {
        "disabled": false,
        "level": "warn",
        "timestamp": true
    },
    "dns": {
        "servers": [
            {
                "tag": "dns_resolver",
                "type": "udp",
                "server": "223.5.5.5",
                "server_port": 53
            },
            {
                "tag": "dns_direct",
                "type": "tls",
                "server": "dns.alidns.com",
                "server_port": 853,
                "domain_resolver": "dns_resolver"
            },
            {
                "tag": "dns_proxy",
                "type": "https",
                "server": "1.1.1.1",
                "path": "/dns-query",
                "server_port": 443,
                "detour": "PROXY"
            },
            {
                "tag": "dns_fakeip",
                "type": "fakeip",
                "inet4_range": "198.18.0.0/15",
                "inet6_range": "fc00::/18"
            }
        ],
        "rules": [
            {
                "action": "route",
                "rule_set": [
                    "geosite-geolocation-cn",
                    "geosite-win-update",
                    "geosite-apple"
                ],
                "server": "dns_direct"
            },
            {
                "action": "route",
                "query_type": [
                    "A"
                ],
                "rule_set": [
                    "geosite-google",
                    "geosite-github",
                    "geosite-telegram",
                    "geosite-category-ai-!cn",
                    "geosite-steam",
                    "geosite-gfw"
                ],
                "server": "dns_fakeip"
            },
            {
                "action": "route",
                "query_type": [
                    "A",
                    "CNAME",
                    "HTTPS"
                ],
                "server": "dns_proxy"
            }
        ],
        "final": "dns_proxy",
        "strategy": "ipv4_only",
        "disable_cache": false,
        "disable_expire": false
    },
    "inbounds": [
        {
            "tag": "MIXED-IN",
            "type": "mixed",
            "listen": "0.0.0.0",
            "listen_port": {{ sing_box_mixed_port | int }},
            "udp_timeout": 180,
            "sniff": true
        },
        {
            "tag": "TPROXY-IN",
            "type": "tproxy",
            "listen": "0.0.0.0",
            "listen_port": {{ sing_box_tproxy_port | int }},
            "udp_timeout": 180,
            "sniff": true
        }
    ],
    "outbounds": [],
    "route": {
        "rules": [
            {
                "action": "hijack-dns",
                "port": 53
            },
            {
                "action": "hijack-dns",
                "protocol": "dns"
            },
            {
                "action": "route",
                "port": 22,
                "outbound": "SSH"
            },
            {
                "action": "route",
                "clash_mode": "direct",
                "outbound": "DIRECT"
            },
            {
                "action": "route",
                "clash_mode": "global",
                "outbound": "PROXY"
            },
            {
                "action": "route",
                "rule_set": [
                    "geoip-private",
                    "geosite-private"
                ],
                "outbound": "DIRECT"
            },
            {
                "action": "route",
                "rule_set": "geosite-steam",
