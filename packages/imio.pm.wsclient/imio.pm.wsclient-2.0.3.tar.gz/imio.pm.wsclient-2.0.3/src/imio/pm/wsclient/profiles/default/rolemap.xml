<?xml version="1.0"?>
<rolemap>
  <roles>
    <role name="Manager"/>
    <role name="Member"/>
  </roles>
  <permissions>
    <permission name="WS Client Access"
                acquire="True">
      <role name="Member"/>
      <role name="Manager"/>
    </permission>
    <permission name="WS Client Send"
                acquire="True">
      <role name="Member"/>
      <role name="Manager"/>
    </permission>
  </permissions>
</rolemap>
