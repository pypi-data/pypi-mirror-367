<odoo noupdate="1">
    <record id="cbl_confirm_shipments_cron" forcecreate="True" model="ir.cron">
        <field name="name">CBL: Confirm Shipments</field>
        <field name="active" eval="True" />
        <field name="user_id" ref="base.user_root" />
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False" />
        <field name="model_id" ref="model_delivery_carrier" />
        <field name="state">code</field>
        <field name="code">model.cbl_confirm_shipment_cron()</field>
    </record>
</odoo>
