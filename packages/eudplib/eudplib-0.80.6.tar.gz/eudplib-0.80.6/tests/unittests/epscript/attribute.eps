function test_attribute() {
    var ret = 0;
    const queue = EUDQueue(10)();
    if (queue.length == 0) ret += 1 << 0;
    queue.append(0);
    if (queue.length == 1) ret += 1 << 1;

    const deque = EUDDeque(10)();
    if (deque.length == 0) ret += 1 << 2;
    deque.append(0);
    if (deque.length == 1) ret += 1 << 3;
    return ret;
}
