# Copy this to .mise.local.toml and fill in your real credentials
# .mise.local.toml is automatically ignored by git

[env]
# Rackspace credentials
RACKSPACE_USERNAME = "your-real-username"
RACKSPACE_API_KEY = "your-real-api-key"
RACKSPACE_ACCOUNT = "your-account-number"

# Optional: Override API endpoints for testing
# RACKSPACE_IDENTITY_URL = "https://identity.api.rackspacecloud.com/v2.0"
# RACKSPACE_TICKET_API_URL = "https://demo.ticketing.api.rackspace.com/v2"

# PyPI tokens for publishing
TWINE_USERNAME = "__token__"
TWINE_PASSWORD = "pypi-your-token-here"

# Test PyPI (optional)
TEST_PYPI_TOKEN = "pypi-test-token"

# GitHub tokens (for act or gh cli)
GITHUB_TOKEN = "ghp_your_token_here"