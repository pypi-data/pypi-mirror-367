# Raxodus environment variables
# Copy to .env and update with your values

# Required: Your Rackspace credentials
RACKSPACE_USERNAME=your-username
RACKSPACE_API_KEY=your-api-key

# Optional: Default account number
RACKSPACE_ACCOUNT=123456

# Optional: API endpoints (defaults shown)
# Override these for production vs demo environments
RACKSPACE_AUTH_URL=https://identity.api.rackspacecloud.com
RACKSPACE_TICKET_API_URL=https://demo.ticketing.api.rackspace.com

# Optional: API region (us, uk, etc.)
RACKSPACE_REGION=us

# Optional: Cache configuration
RAXODUS_CACHE_DIR=~/.cache/raxodus
RAXODUS_CACHE_TTL=300