<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Title>CobotAR Protocol</Title>
    <Version>0.9.1</Version>
    <Authors>Rasmus Lunding</Authors>
    <Company>cobotar.org</Company>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
  </PropertyGroup>

  <PropertyGroup>
    <PackageId>cobotar.protocol</PackageId>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/cobotar/protocol</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Google.Protobuf" Version="3.31.1" />
  </ItemGroup>

</Project>
